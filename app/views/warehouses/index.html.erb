<main>
<div>
  <% @warehouses.each do |w| %>
    <p>Galpão <%= w.code %></p>
    <p>Nome: <%= link_to w.name, warehouse_path(w.id) %></p>
    <p>Cidade: <%= w.city %></p>
    <p>Área: <%= w.area %> m2</p>
  <% end %>
</div>
<div>
  <% if @warehouses.empty? %>
    <p>Nenhum galpão cadastrado</p>
  <% end %>
</div>
</main>